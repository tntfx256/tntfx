{
  "name": "tntfx",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "yarn lint:types && yarn lint:code && yarn lint:prettier",
    "lint:types": "tsc -p tsconfig.json --noEmit",
    "lint:code": "eslint . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier . --write",
    "test": "jest",
    "gen": "yarn workspace @tntfx/cli run gen",
    "gen:icon": "yarn workspace @tntfx/icon run gen"
  },
  "workspaces": [
    "*",
    "configs/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@1.22.19"
}
